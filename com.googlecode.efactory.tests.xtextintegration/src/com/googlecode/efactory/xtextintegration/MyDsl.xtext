grammar com.googlecode.efactory.xtextintegration.MyDsl with org.eclipse.xtext.common.Terminals

generate myDsl "http://www.googlecode.com/efactory/xtextintegration/MyDsl"

Model:
	'Greetings' name=ID
	('REF' refModel = [Model])?
	('REFs' (refModels += [Model])+)?
	greetings+=Greeting*;
	
Greeting:
	'Hello' name=ID '!';
